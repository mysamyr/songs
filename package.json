    "express": "^5.0.1",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "mongoose": "^8.8.3",
    "nodemailer": "^6.9.16",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "winston": "^3.17.0"
