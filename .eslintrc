{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "prettier"
  ],
  "rules": {
    "no-console": "error",
    "no-undef": "error",
    "semi": "error",
    "semi-spacing": "error",
    "eqeqeq": "warn",
    "object-shorthand": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}